{
  "name": "GeoFeatures",
  "version": "0.2.6",
  "summary": "GeoFeatures is a full Geometry library for working with Points, Polygons, LineStrings, etc.",
  "homepage": "https://github.com/tonystone",
  "license": "Apache 2.0",
  "authors": "Tony Stone",
  "source": {
    "git": "https://github.com/tonystone/geofeatures.git",
    "tag": "0.2.6"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/*",
  "public_header_files": "Pod/*.h",
  "frameworks": "MapKit",
  "xcconfig": {
    "GCC_C_LANGUAGE_STANDARD": "c11",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++0x",
    "CLANG_CXX_LIBRARY": "libc++",
    "OTHER_LDFLAGS": "-lc++"
  },
  "subspecs": [
    {
      "name": "Internal",
      "subspecs": [
        {
          "name": "boost",
          "header_mappings_dir": "Pod/Internal",
          "source_files": "Pod/Internal/boost/**/*.{h,hpp,ipp}",
          "public_header_files": "Pod/Internal/boost/**/*.{h,hpp}"
        },
        {
          "name": "detail",
          "dependencies": {
            "GeoFeatures/Internal/boost": [

            ]
          },
          "header_mappings_dir": "Pod/Internal/detail",
          "source_files": [
            "Pod/Internal/detail/*",
            "Pod/Internal/detail/geofeatures/**/*"
          ]
        }
      ]
    }
  ]
}
